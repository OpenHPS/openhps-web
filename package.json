    "prettier": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "shx": "^0.3.4",
    "standard-version": "^9.5.0",
    "ts-node": ">=10.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4"
  }
